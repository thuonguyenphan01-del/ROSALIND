   "source": [
    "Kết quả cuối cùng là tích của n × (n-1) × ... × (n-k+1)."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "4bbdebe2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "936000\n"
     ]
    }
   ],
   "source": [
    "def partial_permutation(n, k):\n",
    "    result = 1\n",
    "    for i in range(n, n-k, -1):  # nhân từ n xuống đến n-k+1\n",
    "        result *= i\n",
    "    return result % 1000000\n",
    "n, k = 80, 9\n",
    "print(partial_permutation(n, k))"
   ]
  }
 ],
